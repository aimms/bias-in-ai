				}
				Procedure pr_pageOpenResultsPage {
					Body: {
						if not p_def_cardID then
							pr_importDataset();
						endif ;
						
						sp_commentToInvestigate := "black is black"  ; ! "<please replace this with \nthe comment you want to investigate>" ;
						
						currentInputs -= data { bp_commentInvestigatedIsToxic };
					}
				}
				Procedure pr_actionInvestigateWhetherEnteredCommentIsToxic {
					Body: {
						
						pr_determineToxic();
					}
				}
